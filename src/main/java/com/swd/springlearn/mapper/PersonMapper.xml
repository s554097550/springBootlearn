<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.swd.springlearn.mapper.PersonMapper">

  <insert id="insert" parameterType="com.swd.springlearn.entity.Person">
    INSERT INTO `t_person`(`user_name`,`pass_word`) VALUES (#{userName},#{passWord})
  </insert>
</mapper>